    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jest-preset-preact": "^1.0.0",
    "preact-cli": "^3.0.1",
    "preact-render-spy": "^1.3.0",
    "sirv-cli": "^0.4.6"
  },
  "dependencies": {
    "dom-to-image": "^2.6.0",
    "preact": "^10.4.6",
    "preact-render-to-string": "^5.1.10",
    "preact-router": "^3.2.1"
  },
  "jest": {
    "preset": "jest-preset-preact",
    "setupFiles": [
      "<rootDir>/tests/__mocks__/browserMocks.js",
